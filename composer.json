{
    "name": "phredeye/yorn",
    "description": "Modules in PHP with the `import` and `export` syntax.",
    "keywords": ["php", "modules", "package"],
    "license": "MIT",
    "authors": [
        {
            "name": "Nuno Maduro",
            "email": "enunomaduro@gmail.com"
        },
        {
            "name": "Fred Isaacs",
            "email": "phredeye@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "nunomaduro/pest": "dev-master",
        "symfony/var-dumper": "^4.3.3"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "NunoMaduro\\Yorn\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist"
    },
        "scripts": {
            "pest": "pest",
            "tests": [
                "@pest"
            ]
        
        },
    "scripts-descriptions": {
        "pest": "Run the pest tests against himself.",
        "tests": "Run all tests."
    }
}
